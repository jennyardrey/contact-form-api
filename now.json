{
  "version": 2,
  "name": "nodejs-express",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "build": {
    "env": {
      "username": "@username",
      "password": "@password"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}